{
    "automock": false,
    "moduleFileExtensions": [
        "js"
    ],
    "testRegex": "lib/js/test/.+/.+_test\\.bs\\.js$",
    "transformIgnorePatterns": [
        ""
    ],
    "setupFilesAfterEnv": [
        "<rootDir>/test/helper/jest/specHelper.js"
    ],
    "notify": false,
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage/",
    "coveragePathIgnorePatterns": [
        "<rootDir>/lib/js/src/.+/library/",
        "<rootDir>/lib/js/src/.+/data/",
        "<rootDir>/lib/js/src/.+/interface/",
        "<rootDir>/lib/js/src/.+/type/",
        "<rootDir>/lib/js/src/run/.+/api/",
        "<rootDir>/lib/js/src/run/cloud_picture/domain_layer/domain/shader/",
        "<rootDir>/lib/js/src/.+/ParseDataPipelineDoService.*",
        "<rootDir>/lib/js/src/.+/PointsGeometryCPRepo.*"
    ],
    "collectCoverageFrom": [
        "lib/js/src/**/*.js",
        "!**/node_modules/**",
        "!**/vendor/**"
    ],
    "coverageThreshold": {
        "global": {
            "lines": 95,
            "statements": 95,
            "branches": 80,
            "functions": 92
        }
    }
}